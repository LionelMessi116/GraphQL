{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "declaration": false,
    "rootDir": "./src",
    "baseUrl": ".",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "noUnusedParameters": true
  }
}
