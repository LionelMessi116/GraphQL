{
  "name": "@urql/preact",
  "version": "1.1.0",
  "description": "Urql that works with Preact-core",
  "author": "jdecroock <decroockjovi@gmail.com> (https://twitter.com/JoviDeC)",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./node_modules/.cache",
    "build": "rollup -c rollup.config.js",
    "watch": "rollup -w -c rollup.config.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "run-s clean test build",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoviDeCroock/preact-urql.git"
  },
  "bugs": {
    "url": "https://github.com/JoviDeCroock/preact-urql/issues"
  },
  "homepage": "https://github.com/JoviDeCroock/preact-urql#readme",
  "sideEffects": false,
  "private": false,
  "keywords": [
    "preact",
    "graphql client",
    "state management",
    "cache"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "jest": {
    "preset": "../../scripts/jest/preset"
  },
  "devDependencies": {
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "preact": "^10.1.1"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "preact": ">= 10.0.0"
  },
  "dependencies": {
    "urql": "^1.8.0",
    "wonka": "^4.0.6"
  }
}
